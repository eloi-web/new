{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.js"
    },
    {
      "source": "/admin-dashboard",
      "destination": "/admin-dashboard.html"
    },
    {
      "source": "/login",
      "destination": "/log-in.html"
    },
    {
      "source": "/signup",
      "destination": "/sign-up.html"
    }
  ]
}
